<div class="pages">
  <%= page_entries_info @places, entry_name: 'item' %>
</div>
<% @places.each do |place| %>
  <div class="booyah-box col-10 offset-1">
    <h1><%= place.name %></h1>
    <i><%= place.address %></i><br /><br />
    <p><%= place.description %></p><br />
  </div>
<% end %>
<div class="pages">
  <%= paginate @places %>
</div>